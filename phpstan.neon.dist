includes:
   - phpstan-baseline.neon

parameters:
   level: max
   paths:
      - app
      - tests
   ignoreErrors:
      - '#^Call to an undefined method Aura\\Router\\Map::extras\(\)\.$#'
      - '#^Call to an undefined method Aura\\Router\\Map::tokens\(\)\.$#'
      - '#^Call to an undefined static method Illuminate\\Database\\Capsule\\Manager::.*\(\)\.$#'
